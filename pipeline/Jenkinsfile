pipeline {
    agent any
    triggers {
        pollSCM 'H/15 * * * *'
    }
    stages {
        stage("Determine products and tests to run.") {
            steps {
                script {
                    def diff = sh(returnStdout: true, script: "git diff origin/master...")
                    print(diff)
                }
            }
        }
    }
}